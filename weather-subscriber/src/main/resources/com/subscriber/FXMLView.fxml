<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.GridPane?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.subscriber.SubscriberController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <GridPane>
        <Label text="Temperature" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <TextField fx:id="fTemperature" text="0" maxWidth="80" editable="false" focusTraversable="false"
                   GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <Label text="Pressure" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <TextField fx:id="fPressure" text="0" maxWidth="80" editable="false" focusTraversable="false"
                   GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Label text="Speed Wild" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
        <TextField fx:id="fSpeedWild" text="0" maxWidth="80" editable="false" focusTraversable="false"
                   GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Label text="Humidity" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
        <TextField fx:id="fHumidity" text="0" maxWidth="80" editable="false" focusTraversable="false"
                   GridPane.columnIndex="1" GridPane.rowIndex="3"/>

    </GridPane>
</VBox>